void setup() 
{
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);  // here the 0x3c is the I2C address, check your i2c address if u have multiple devices.
  display.clearDisplay();
  delay(2000);
  pinMode(LEFTBUTTON,INPUT);
  pinMode(RIGHTBUTTON,INPUT);
  
}

void loop() 
{
  left=digitalRead(LEFTBUTTON);
  right=digitalRead(RIGHTBUTTON);
  if(left==LOW&&right==HIGH){
    display.clearDisplay();
    display.drawBitmap(0,0,_leftpawontable, 128, 40, 1);
    display.display();
    delay(50);
  }
  if(left==HIGH&&right==LOW){
    display.clearDisplay();
    display.drawBitmap(0,0,_rightpawontable , 128, 40, 1);
    display.display();
    delay(50);
  }
  if(left==LOW&&right==LOW){
    display.clearDisplay();
    display.drawBitmap(0,0,_pawsontable , 128, 40, 1);
    display.display();
    delay(50);
  }
  if(left==HIGH&&right==HIGH){
    display.clearDisplay();
    display.drawBitmap(0,0,_pawsonair , 128, 40, 1);
    display.display();
    delay(50);
  }
  display.display();
}